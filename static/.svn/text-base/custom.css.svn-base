.tc-pagination-wrap {
    font-size: 0;
    margin: 30px 0;
    text-align: center
}

.pagination-pages li.pagination-next.last-page,.pagination-pages li.pagination-pre.first-page {
    display: none
}

.pagination-pages li {
    display: inline-block;
    margin-left: 10px
}

.pagination-pages li>a {
    display: block;
    width: 35px;
    height: 35px;
    line-height: 35px;
    font-size: 14px;
    color: #666;
    text-align: center;
    border: 1px solid #ddd;
    box-sizing: border-box
}

.pagination-pages li:first-child {
    margin-left: 0
}

.pagination-pages li>a:hover {
    color: #2b2b2b;
    border: 1px solid #999
}

.pagination-pages li.current>a {
    border: none;
    color: #333;
    cursor: default;
    font-weight: 700
}

.pagination-pages li.disabled>a {
    border: none;
    color: #666;
    cursor: default
}

.pagination-pages li.pagination-next>a,.pagination-pages li.pagination-pre>a {
    width: auto;
    padding: 0 8px
}

.pagination-pages li.pagination-next>a i {
    display: inline-block;
    margin-left: 2px;
    position: relative;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 0 5px 5px;
    border-color: transparent #666
}

.pagination-pages li.pagination-next>a i:after {
    content: '';
    position: absolute;
    left: -6px;
    top: -5px;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 0 5px 5px;
    border-color: transparent #fff
}

.pagination-pages li.pagination-next>a:hover i,.pagination-pages li.pagination-pre>a:hover i {
    border-color: transparent #2b2b2b
}

.pagination-pages li.pagination-pre>a i {
    display: inline-block;
    margin-right: 2px;
    position: relative;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 5px 5px 0;
    border-color: transparent #666
}

.pagination-pages li.pagination-pre>a i:after {
    content: '';
    position: absolute;
    right: -6px;
    top: -5px;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 5px 5px 0;
    border-color: transparent #fff
}

